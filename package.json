{
  "name": "elections-webui",
  "private": true,
  "version": "2.0.0",
  "description": "A Web user interface for Guild elections Graphics",
  "author": "Rob Sumner <robfsumner@gmail.com> (http://robertsumner.co.uk)",
  "scripts": {
    "start": "NODE_ENV=production ./node_modules/.bin/babel-node ./server.js",
    "dev": "NODE_ENV=development nodemon --ignore dumps/ --exec babel-node ./server.js",
    "lint": "NODE_ENV=test ./node_modules/.bin/eslint --ext .js .",
    "migrate": "NODE_ENV=development sequelize --config=sequelize.json db:migrate",
    "webpack": "webpack-dev-server --progress --colors --public --disable-host-check --host 0.0.0.0",
    "dist": "NODE_ENV=production webpack -p --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GuildTV/elections-webui.git"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.15.2",
    "casparcg-connection": "https://github.com/julusian/casparcg-connection",
    "cors": "^2.8.1",
    "deep-equal": "^1.0.1",
    "eslint": "^3.9.1",
    "eslint-plugin-react": "^6.5.0",
    "express": "^4.14.0",
    "md5": "^2.2.1",
    "mysql": "^2.15.0",
    "mysql2": "^1.1.1",
    "promise-map-series": "^0.2.3",
    "request": "^2.79.0",
    "sequelize": "^3.24.8",
    "sequelize-cli": "^2.4.0",
    "socket.io": "^2.0.4",
    "underscore": "^1.13.2",
    "uuid": "^3.1.0",
    "xml2js": "^0.4.17",
    "xmlbuilder": "^8.2.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
